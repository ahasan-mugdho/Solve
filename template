#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
typedef vector<int> vi;
typedef vector<ll> vl;
#define endl '\n'
#define yes cout << "YES" << endl
#define no cout << "NO" << endl
#define pb push_back
#define all(a) (a).begin(), (a).end()
#define rall(a) (a).rbegin(), (a).rend()
#define sz(x) (int)x.size()

#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
ll gcd(ll a,ll b) {return __gcd(a, b);}
ll lcm(ll a,ll b) {return a*(b/gcd(a,b));}
int64_t ceil_div(int64_t a, int64_t b)
{
  return (a+b-1)/b;
}
ll power(ll a, ll b)
{
  ll result = 1;
  while(b) {
    if(b%2) {
      result *= a;
      b--;
    } else {
      a *= a;
      b /= 2;
    }
  }
  return result;
}
bool is_prime(ll x)
{
  if(x<2)return 0;
  if(x==2)return 1;
  if(x%2==0)return 0;
  for(ll i=3;i*i<= x;i+=2)
  {
    if(x%i==0)
      return 0;
  }
  return 1;
}
int dx[]={1,-1,0,0};int dy[]={0,0,1,-1};


int main() 
{
  optimize();
  ll tst,n,i;
  cin>>tst;
  start:
  while(tst--)
  {

        cin>>n;
        vector<ll> v(n);
        ll e=0,ld=0;
        vector<ll> ans;
        for(int i=0;i<n;i++){
            cin>>v[i];
            if(v[i]%2==0){
                e++;
                ans.push_back(v[i]);
               
            }
        }
        if(e>=2){
            cout<<ans[0]<<" "<<ans[1]<<endl;
            continue;
        }
        
        for(int i=0;i<n;i++){
           for(int j=i+1;j<n&&j<i+100;j++){
            if((v[j]%v[i])%2==0){
                cout<<v[i]<<" "<<v[j]<<endl;
                goto start;
            }
           }
        }
       cout<<-1<<endl;
  }
  return 0;
}
